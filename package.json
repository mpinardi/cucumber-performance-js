{
  "name": "cucumber-perf",
  "version": "0.5.0",
  "description": "The js implimentation of Cucumber-Perf",
  "keywords": [
    "testing",
    "bdd",
    "cbdd",
    "cucumber",
    "gherkin",
    "tests",
    "performance"
  ],
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib/index",
  "engines": {
    "node": ">=0.10"
  },
  "bin": {
    "cucumber-perf": "./bin/cucumber-perf"
  },
  "scripts": {
    "feature-test": "node ./bin/cucumber-perf",
    "build": "babel src -d lib",
    "build-local": "babel src -d lib --ignore '**/*_spec.js'",
